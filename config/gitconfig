[color]
	branch  = auto
	diff = auto
	status = auto
[alias]
	br = branch
	co = checkout
	ci = commit
	df = diff
	st = status -sb -uno
	dc = diff --cached
	gl = log --pretty=short --decorate=short --graph
	lc = log ORIG_HEAD.. --stat --no-merges
	ca = commit --amend
	nb = checkout -b
	cp = cherry-pick
	review = !GIT_CURRENT_BRANCH=$(git name-rev --name-only HEAD) && git log -p origin/$GIT_CURRENT_BRANCH..$GIT_CURRENT_BRANCH
[color "status"]
	added = yellow
	changed = green
[color "diff"]
	meta = cyan bold
	frag = magenta bold
	old = red bold
	new = green bold
[github]
	user = markstory
[user]
	name = Mark Story
	email = mark@mark-story.com
	signingkey = 55B3AEA0
[push]
	default = current
